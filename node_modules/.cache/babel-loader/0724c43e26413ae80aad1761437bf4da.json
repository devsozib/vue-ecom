AAc;AACZ,YAAI,QAAQ,CAAC,MAAT,IAAmB,GAAvB,EAA4B;AAC1B,UAAA,IAAI,CAAC;AACH,YAAA,IAAI,EAAE,4BADH;AAEH,YAAA,IAAI,EAAE,SAFH;AAGH,YAAA,mBAAmB,EAAE;AAHlB,WAAD,CAAJ,CAD0B,CAM1B;;AACA,UAAA,MAAI,CAAC,KAAL,CAAW,WAAX;AACF;AACD,OAhBL,EAiBI,UAAC,KAAD,EAAW;AACT,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACF,OAnBJ;AAqBD,KA/CM;AAiDP,IAAA,aAjDO,2BAiDS;AAAA;;AACd,MAAA,KAAK,CAAC,GAAN,WAAa,KAAK,OAAlB,yBAAwC,KAAK,KAA7C,GAAsD,IAAtD,CACE,UAAC,QAAD,EAAc;AACZ,YAAI,QAAQ,CAAC,MAAT,KAAoB,GAAxB,EAA6B;AAC3B,UAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,OAAlB;AACF;AACD,OALH,EAME,UAAC,KAAD,EAAW;AACT,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACF,OARF;AAUD;AA5DM,GAbI;AA2Eb,EAAA,OA3Ea,qBA2EH;AAAA;;AACR,SAAK,EAAL,GAAU,KAAK,MAAL,CAAY,MAAZ,CAAmB,EAA7B;AACA,SAAK,OAAL,GAAe,KAAK,QAAL,CAAc,IAAd,CAAmB,UAAC,OAAD;AAAA,aAAa,OAAO,CAAC,EAAR,IAAc,MAAI,CAAC,EAAhC;AAAA,KAAnB,CAAf;AACA,SAAK,QAAL,GAAgB,KAAK,UAAL,CAAgB,IAAhB,CACd,UAAC,QAAD;AAAA,aAAc,QAAQ,CAAC,EAAT,IAAe,MAAI,CAAC,OAAL,CAAa,UAA1C;AAAA,KADc,CAAhB;AAGA,SAAK,KAAL,GAAa,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAb;AACD;AAlFY,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row pt-5\">\n      <div class=\"col-md-1\"></div>\n      <div class=\"col-md-4 col-12\">\n        <img :src=\"product.imageURL\" :alt=\"product.name\" class=\"img-fluid\" />\n      </div>\n      <div class=\"col-md-6 col-12 pt-3 pt-md-0\">\n        <h4>{{ product.name }}</h4>\n        <h6 class=\"category font-italic\">{{ category.categoryName }}</h6>\n        <h6 class=\"font-weight-bold\">$ {{ product.price }}</h6>\n        <p>\n          {{ product.description }}\n        </p>\n\n        <div class=\"d-flex flex-row justify-content-between\">\n          <div class=\"input-group col-md-3 col-4 p-0\">\n            <div class=\"input-group-prepend\">\n              <span class=\"input-group-text\" id=\"basic-addon1\">Quantity</span>\n            </div>\n            <input class=\"form-control\" type=\"number\" v-bind:value=\"quantity\" />\n          </div>\n\n          <div class=\"input-group col-md-3 col-4 p-0\">\n            <button\n              type=\"button\"\n              id=\"add-to-cart-button\"\n              class=\"btn\"\n              @click=\"addToCart(this.id)\"\n            >\n              Add to Cart\n              <ion-icon name=\"cart-outline\" v-pre></ion-icon>\n            </button>\n          </div>\n        </div>\n\n        <div class=\"features pt-3\">\n          <h5><strong>Features</strong></h5>\n          <ul>\n            <li>Lorem ipsum dolor sit amet consectetur adipisicing elit.</li>\n            <li>Officia quas, officiis eius magni error magnam voluptatem</li>\n            <li>nesciunt quod! Earum voluptatibus quaerat dolorem doloribus</li>\n            <li>molestias ipsum ab, ipsa consectetur laboriosam soluta et</li>\n            <li>ut doloremque dolore corrupti, architecto iusto beatae.</li>\n          </ul>\n        </div>\n\n        <button\n          id=\"wishlist-button\"\n          class=\"btn mr-3 p-1 py-0\"\n          :class=\"{ product_added_wishlist: isAddedToWishlist }\"\n          @click=\"addToWishList(this.id)\"\n        >\n          {{ wishlistString }}\n        </button>\n        <button\n          id=\"show-cart-button\"\n          type=\"button\"\n          class=\"btn mr-3 p-1 py-0\"\n          @click=\"listCartItems()\"\n        >\n          Show Cart\n\n          <ion-icon name=\"cart-outline\" v-pre></ion-icon>\n        </button>\n      </div>\n      <div class=\"col-md-1\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      product: {},\n      category: {},\n      id: null,\n      token: null,\n      isAddedToWishlist: false,\n      wishlistString: \"Add to wishlist\",\n      quantity: 1,\n    };\n  },\n  props: [\"baseURL\", \"products\", \"categories\"],\n  methods: {\n    addToWishList(productId) {\n      axios\n        .post(`${this.baseURL}wishlist/add?token=${this.token}`, {\n          id: productId,\n        })\n        .then(\n          (response) => {\n            if (response.status == 201) {\n              this.isAddedToWishlist = true;\n              this.wishlistString = \"Added to WishList\";\n            }\n          },\n          (error) => {\n            console.log(error);\n          }\n        );\n    },\n    addToCart(productId) {\n      if (!this.token) {\n        swal({\n          text: \"Please log in first!\",\n          icon: \"error\",\n        });\n        return;\n      }\n      axios\n        .post(`${this.baseURL}cart/add?token=${this.token}`, {\n          productId: productId,\n          quantity: this.quantity,\n        })\n        .then(\n          (response) => {\n            if (response.status == 201) {\n              swal({\n                text: \"Product Added to the cart!\",\n                icon: \"success\",\n                closeOnClickOutside: false,\n              });\n              // refresh nav bar\n              this.$emit(\"fetchData\");\n            }\n          },\n          (error) => {\n            console.log(error);\n          }\n        );\n    },\n\n    listCartItems() {\n      axios.get(`${this.baseURL}cart/?token=${this.token}`).then(\n        (response) => {\n          if (response.status === 200) {\n            this.$router.push(\"/cart\");\n          }\n        },\n        (error) => {\n          console.log(error);\n        }\n      );\n    },\n  },\n  mounted() {\n    this.id = this.$route.params.id;\n    this.product = this.products.find((product) => product.id == this.id);\n    this.category = this.categories.find(\n      (category) => category.id == this.product.categoryId\n    );\n    this.token = localStorage.getItem(\"token\");\n  },\n};\n</script>\n\n<style>\n.category {\n  font-weight: 400;\n}\n\n/* Chrome, Safari, Edge, Opera */\ninput::-webkit-outer-spin-button,\ninput::-webkit-inner-spin-button {\n  -webkit-appearance: none;\n  margin: 0;\n}\n\n/* Firefox */\ninput[type=\"number\"] {\n  -moz-appearance: textfield;\n}\n\n#add-to-cart-button {\n  background-color: #febd69;\n}\n\n#wishlist-button {\n  background-color: #b9b9b9;\n  border-radius: 0;\n}\n\n#show-cart-button {\n  background-color: #131921;\n  color: white;\n  border-radius: 0;\n}\n</style>\n"],"sourceRoot":""}]}